<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="images/favicon.ico" type="image/ico" />

    <title>Book Store</title>

    <!-- Bootstrap -->
    <link href="/Assets/Admin/layout/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="/Assets/Admin/layout/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="/Assets/Admin/layout/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="/Assets/Admin/layout/vendors/iCheck/skins/flat/green.css" rel="stylesheet">

    <!-- bootstrap-progressbar -->
    <link href="/Assets/Admin/layout/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="/Assets/Admin/layout/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
    <!-- bootstrap-daterangepicker -->
    <link href="/Assets/Admin/layout/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">

    <!-- Custom Theme Style -->
    <link href="/Assets/Admin/layout/build/css/custom.min.css" rel="stylesheet">
</head>

<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                @RenderPage("_LeftCol.cshtml")
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    @{ Html.RenderAction("NameLogin", "Home");}
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">
                                    @*<li><a href="javascript:;"> Profile</a></li>
                                        <li>
                                            <a href="javascript:;">
                                                <span class="badge bg-red pull-right">50%</span>
                                                <span>Settings</span>
                                            </a>
                                        </li>
                                        <li><a href="javascript:;">Help</a></li>*@
                                    <li><a href="/Admin/Login/Index"><i class="fa fa-sign-out pull-right"></i> Đăng Xuất</a></li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>

            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">

                <div class="page-title">
                    <div class="title_left">
                        <h3>@RenderSection("header", required: true)</h3>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <!--Thêm alert-->

                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <h2>@RenderSection("rowtitle", required: false)</h2>
                            <div class="x_content">
                                <br>
                                @if (TempData["AlertMessage"] != null)
                                {
                                    <div class="alert @TempData["AlertType"] hide" id="alertMess">@TempData["AlertMessage"]</div>
                                }
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    Copyright ©
                    <script>document.write(new Date().getFullYear());</script> | This website made by Sanero and  ThucAnhNguyeen
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    <!-- jQuery -->
    <script src="/Assets/Admin/layout/vendors/jquery/dist/jquery.min.js"></script>

    <script src="/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <!-- Bootstrap -->
    <script src="/Assets/Admin/layout/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="/Assets/Admin/layout/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="/Assets/Admin/layout/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="/Assets/Admin/layout/vendors/Chart.js/dist/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="/Assets/Admin/layout/vendors/gauge.js/dist/gauge.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="/Assets/Admin/layout/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="/Assets/Admin/layout/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="/Assets/Admin/layout/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="/Assets/Admin/layout/vendors/Flot/jquery.flot.js"></script>
    <script src="/Assets/Admin/layout/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="/Assets/Admin/layout/vendors/Flot/jquery.flot.time.js"></script>
    <script src="/Assets/Admin/layout/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="/Assets/Admin/layout/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="/Assets/Admin/layout/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="/Assets/Admin/layout/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="/Assets/Admin/layout/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="/Assets/Admin/layout/vendors/DateJS/build/date.js"></script>
    <!-- JQVMap -->
    <script src="/Assets/Admin/layout/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="/Assets/Admin/layout/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="/Assets/Admin/layout/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="/Assets/Admin/layout/vendors/moment/min/moment.min.js"></script>
    <script src="/Assets/Admin/layout/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!--Plugin-->
    <script src="/Assets/Admin/js/plugin/ckfinder/ckfinder.js"></script>
    <script src="/Assets/Admin/CustomJS.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="/Assets/Admin/layout/build/js/custom.min.js"></script>
    @RenderSection("jsFooter", required: false);
</body>
</html>
